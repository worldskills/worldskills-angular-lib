{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "my-lib": [
        "./dist/my-lib"
      ],
      "worldskills-angular-lib": [
        "dist/worldskills-angular-lib/worldskills-angular-lib",
        "dist/worldskills-angular-lib",
        "projects/worldskills-angular-lib/src/public_api.ts"
      ]
    },
    "useDefineForClassFields": false
  }
}
